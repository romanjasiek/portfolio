t.left; \n            var y = e.clientY - rect.top;\n            element.style = \"--mouse-x:\" + (x / element.offsetWidth) + \";--mouse-y:\" + (y / element.offsetHeight) + \";\";\n        });\n        element.addEventListener(\"mouseleave\", function(e) {\n            element.style = \"\";\n        });\n    }\n}\n\nif (document.readyState === \"complete\" || document.readyState === \"interactive\") {\n    setTimeout(addMousePositionToCss, 1);\n} else {\n    document.addEventListener(\"DOMContentLoaded\", addMousePositionToCss, false);\n}\n\nexport default Skills;","export default __webpack_public_path__ + \"static/media/agadir.2595c235.jpeg\";","export default __webpack_public_path__ + \"static/media/barcelona.e665cdd0.jpeg\";","export default __webpack_public_path__ + \"static/media/chimp.f5c27b2f.jpeg\";","export default __webpack_public_path__ + \"static/media/cruiseships.cf5bea24.jpeg\";","export default __webpack_public_path__ + \"static/media/lanzarote.d8d4bae6.jpeg\";","export default __webpack_public_path__ + \"static/media/oslo.dbe6faea.jpeg\";","export default __webpack_public_path__ + \"static/media/vnvnation.1ae428c3.jpeg\";","export default __webpack_public_path__ + \"static/media/agadir2.ebc893a3.jpeg\";","export default __webpack_public_path__ + \"static/media/cannes.2b117304.jpeg\";","export default __webpack_public_path__ + \"static/media/corsica.6a3527f4.jpeg\";","export default __webpack_public_path__ + \"static/media/monkeys.20e76518.jpeg\";","export default __webpack_public_path__ + \"static/media/ocean.b0b70afd.jpeg\";","export default __webpack_public_path__ + \"static/media/vaticancity.1f642aa9.jpeg\";","export default __webpack_public_path__ + \"static/media/wismar.93a7c0b4.jpeg\";","import React from 'react';\n\nimport Agadir from '../img/gallery/agadir.jpeg';\nimport Barcelona from '../img/gallery/barcelona.jpeg';\nimport Chimp from '../img/gallery/chimp.jpeg';\nimport CruiseShips from '../img/gallery/cruiseships.jpeg';\nimport Lanzarote 